{
    "name": "wws-aslan/pdf-version-converter",
    "description": "PHP library for converting the version of PDF files (for compatibility purposes).",
    "keywords": ["pdf", "converter", "version converter", "pdf version converter", "pdf version"],
    "license": "MIT",
    "type": "library",
    "minimum-stability" : "stable",
    "authors": [
        {
            "name": "Thiago Rodrigues",
            "email": "xthiago@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xthiago\\PDFVersionConverter\\": [
                "src/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/filesystem": "^4|^5",
        "symfony/process": "^4|^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
